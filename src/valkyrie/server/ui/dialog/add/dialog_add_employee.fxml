<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox xmlns:fx="http://javafx.com/fxml" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity" spacing="15.0" xmlns="http://javafx.com/javafx"
      fx:controller="valkyrie.server.ui.dialog.add.AddEmployeeController"
      stylesheets="@../../../resources/dark_theme.css">
   <children>
      <TextField fx:id="tfFirstName" prefWidth="200.0" promptText="First Name">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <TextField fx:id="tfLastName" prefWidth="200.0" promptText="Last Name">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <HBox alignment="BOTTOM_RIGHT" prefHeight="50.0" spacing="10.0">
         <children>
            <Button fx:id="btnOk" minHeight="30.0" minWidth="75.0" mnemonicParsing="false" text="OK" />
            <Button fx:id="btnCancel" minHeight="30.0" minWidth="75.0" mnemonicParsing="false" text="Cancel" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
