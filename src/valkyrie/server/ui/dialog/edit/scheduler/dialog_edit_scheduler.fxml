<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane xmlns:fx="http://javafx.com/fxml" hgap="20.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
          minWidth="-Infinity" stylesheets="@../../../../resources/dark_theme.css" vgap="10.0"
          xmlns="http://javafx.com/javafx"
          fx:controller="valkyrie.server.ui.dialog.edit.scheduler.EditSchedulerDialogController">
  <columnConstraints>
    <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" minWidth="10.0" />
    <ColumnConstraints halignment="LEFT" hgrow="ALWAYS" minWidth="10.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <Label text="Tag" />
      <TextField fx:id="tfTag" prefWidth="175.0" GridPane.columnIndex="1" />
      <Label text="Device IP" GridPane.rowIndex="1" />
      <TextField fx:id="tfDeviceIP" prefWidth="175.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <HBox alignment="BOTTOM_RIGHT" prefHeight="40.0" spacing="10.0" GridPane.columnSpan="2" GridPane.rowIndex="2">
         <children>
            <Button fx:id="btnOk" minHeight="30.0" minWidth="75.0" mnemonicParsing="false" text="OK" />
            <Button fx:id="btnCancel" minHeight="30.0" minWidth="75.0" mnemonicParsing="false" text="Cancel" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</GridPane>
